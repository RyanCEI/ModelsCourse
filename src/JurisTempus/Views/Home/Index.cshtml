@{
    ViewData["Title"] = "Home Page";
}
@model ClientViewModel[]

  <div class="row">
    <div class="col-8 offset-2">
      <h3>Clients</h3>
      @foreach (var client in Model)
      {
        <div>@client.Name - @client.Phone</div>
        <div>@client.CityTown - @client.StateProvince</div>
        <div>
          <ul>
            @foreach (var clientCase in client.Cases)
            {
              <li>@clientCase.FileNumber</li>
            }
        </ul>
          </div>
      }
    </div>
  </div>

